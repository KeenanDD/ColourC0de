{
  "name": "ColourC0de",
  "version": "1.0.5",
  "description": "Change viewport to suit vision needs or for more creative options",
  "author": "Keenan Dobson",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "browser_action": {
    "default_title": "ColourC0de",
    "default_popup": "/popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/content/script.js"
      ]
    }
  ]
}
